<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <title>{% block title %}Bioversity International TIP{% endblock %}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    
    {% block stylesheets %}
      {% stylesheets
        'css/bootstrap-responsive.css'
        'css/bootstrap.css'
        'css/style.css'
        'css/new_style.css'
      %}
          <link href="{{ asset_url }}" rel="stylesheet" type="text/css">
      {% endstylesheets %}
    {% endblock %}
    
    <script type="text/javascript">
      var dev_stage= '{% if(app.environment == 'dev') %}/app_dev.php{% endif %}';      
    </script>
    
    {% block javascripts %}
      {% javascripts 
          'js/jquery/jquery.js'
          'js/jquery/jquery-ui-1.9.2.custom.js'
          'js/bootstrap/bootstrap-transition.js'
          'js/bootstrap/bootstrap-alert.js'
          'js/bootstrap/bootstrap-modal.js'
          'js/bootstrap/bootstrap-dropdown.js'
          'js/bootstrap/bootstrap-scrollspy.js'
          'js/bootstrap/bootstrap-tab.js'
          'js/bootstrap/bootstrap-tooltip.js'
          'js/bootstrap/bootstrap-popover.js'
          'js/bootstrap/bootstrap-button.js'
          'js/bootstrap/bootstrap-collapse.js'
          'js/bootstrap/bootstrap-carousel.js'
          'js/bootstrap/bootstrap-typeahead.js'
          'js/tooltip.js'
      %}
          <script type="text/javascript" src="{{ asset_url }}"></script>
      {% endjavascripts  %}
    {% endblock %}
    
    <!-- Le HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9] -->
    <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    <link rel="stylesheet" href="http://code.jquery.com/ui/1.10.0/themes/base/jquery-ui.css" />
    
    <script src="http://code.jquery.com/jquery-1.8.3.js"></script>
    <script src="http://code.jquery.com/ui/1.10.0/jquery-ui.js"></script>
    <script type="text/javascript" src="http://jzaefferer.github.com/jquery-validation/jquery.validate.js"></script>
    
  </head>
  <body>
    
   {% include '::mainMenu.html.twig' %}
      
    <div id="content_page">
      {% block menuhead %}{% endblock %}
      <div id="content_body">
        {% block body %}{% endblock %}
        {% block footer %}{% endblock %}
      </div>
    </div>
    
  </body>
</html>
