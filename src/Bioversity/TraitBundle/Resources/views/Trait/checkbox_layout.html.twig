{% block form_row %}
    <input type="checkbox"
           id="{{ form.name }}_enabler"
           name="{{ form.name }}_enabler"
           title="enable field"
           value="enable"
           onclick="javascript: enableField(this, '{{ form.name }}');"/>

    {{ form_label(form) }}
    {{ form_errors(form) }}
    {{ form_widget(form) }}
{% endblock %}