<?php

namespace Bioversity\AttributefieldsBundle\Document;

use Doctrine\ODM\MongoDB\Mapping\Annotations as MongoDB;

/**
 * @MongoDB\Document
 */
class Term
{

  /**
   * @MongoDB\Id
   */
  protected $id;
  
  /**
   * @MongoDB\String
   */
  protected $_idx;
  
  /**
   * @MongoDB\String
   */
  protected $_ns;
  
  /**
   * @MongoDB\String
   */
  protected $_code;
  
  /**
   * @MongoDB\String
   */
  protected $label;
  
  /**
   * @MongoDB\String
   */
  protected $description;

  /**
   * Set custom_field
   *
   * @param string $field
   * @param string $value
   * @return Attribute
   */
  public function setCustomField($field, $value)
  {
      $this->$field = $value;
      return $this;
  }
  
}
