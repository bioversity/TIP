<?php

namespace Bioversity\AttributefieldsBundle\Document;

use Doctrine\ODM\MongoDB\Mapping\Annotations as MongoDB;

/**
 * @MongoDB\Document
 */
class Edge
{

  /**
   * @MongoDB\Id
   */
  protected $id;
  
  /**
   * @MongoDB\Int
   */
  protected $_subject;
  
  /**
   * @MongoDB\String
   */
  protected $_predicate;
  
  /**
   * @MongoDB\Int
   */
  protected $_object;

  /**
   * Set custom_field
   *
   * @param string $field
   * @param string $value
   * @return Attribute
   */
  public function setCustomField($field, $value)
  {
      $this->$field = $value;
      return $this;
  }
  
}
